{
  "name": "personal_website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "python3 gen_page.py dev; vite",
    "build": " python3 gen_page.py build; vite build; mv temp_dist/* dist/; rm -rf temp_dist",
    "preview": "vite preview",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "predeploy": "npm run build; cp legacy/* dist/",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "dompurify": "^3.3.1",
    "highlight.js": "^11.11.1",
    "lucide-vue-next": "^0.556.0",
    "marked": "^17.0.1",
    "marked-highlight": "^2.2.3",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "prettier": "3.6.2",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5"
  }
}
